{
  "extends": "tslint-config-airbnb",
  "rules": {
    "no-boolean-literal-compare": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "variable-name": [
      true,
      "check-format",
      "allow-pascal-case"
    ],
    "import-name": [true, { "react": "React" }]
  }
}